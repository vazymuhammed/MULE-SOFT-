<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>MovieJavaApplication</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>databaseCreation</servlet-name>
    <servlet-class>myPackage.MoviesDatabaseCreation</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>insertMovie</servlet-name>
    <servlet-class>myPackage.InsertMovieList</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getMovie</servlet-name>
    <servlet-class>myPackage.GetMovieDetails</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>databaseCreation</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>insertMovie</servlet-name>
    <url-pattern>/insert</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getMovie</servlet-name>
    <url-pattern>/getDetails</url-pattern>
  </servlet-mapping>
</web-app>